
<h1>Foods#index</h1>
<p>Find me in app/views/foods/index.html.erb</p>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>

<table>
 <thead>
  <tr>
   <th>food_name</th>
   <th>evaluation</th>
   <th>review</th>
   <th colspan="3"></th>
  </tr>
 </thead>

<script type="text/javascript">

 $(function(){
   $('aa').onselect("すごく美味しい")
 });

</script>

 <tbody>
  <% @foods.each do |food| %>
  <tr>
    <td><%= food.food_name %></td>
    <td><%= food.evaluation %></td>
    <td><%= food.review %></td>
    <td><%= link_to 'New', new_food_path %></td>
    <td><%= link_to 'Show', food %></td>
    <td><%= link_to 'Edit', edit_food_path(food) %></td>
    <td><%= link_to 'Destroy', food, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
  <% end %>
 </tbody>
</table>


 
